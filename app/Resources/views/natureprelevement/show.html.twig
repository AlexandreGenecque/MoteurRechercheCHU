{% extends 'base.html.twig' %}

{% block body %}
    <h1>Nature Prélévement</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ naturePrelevement.id }}</td>
            </tr>
            <tr>
                <th>Nom nature prélévement</th>
                <td>{{ naturePrelevement.nomNaturePrelevement }}</td>
            </tr>
            <tr>
                <th>Precision prélévement</th>
                <td>{{ naturePrelevement.precisionPrelevement }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('natureprelevement_index') }}">Retour à la liste</a>
        </li>
        <li>
            <a href="{{ path('natureprelevement_edit', { 'id': naturePrelevement.id }) }}">Modifier</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Supprimer">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
